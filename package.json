{
  "name": "@ch99q/twc",
  "version": "0.1.1",
  "description": "TradingView WebSocket Client",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./studies": {
      "types": "./dist/studies/index.d.ts",
      "default": "./dist/studies/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch",
    "prepublishOnly": "bun run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "tradingview",
    "websocket",
    "client",
    "trading",
    "real-time",
    "data",
    "chart",
    "series",
    "screener"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.5.4",
    "@types/ws": "^8.18.1",
    "bun-types": "latest",
    "dotenv": "^17.2.3",
    "tsdown": "^0.15.6",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "bun@1.2.23",
  "dependencies": {
    "ws": "^8.18.3"
  }
}
